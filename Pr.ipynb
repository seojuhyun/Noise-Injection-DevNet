    "# append 0\n",
    "\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import os\n",
    "import random\n",
    "from sklearn.model_selection import train_test_split\n",
    "import shutil\n",
    "import tensorflow as tf\n",
    "\n",
    "os.environ[\"CUDA_VISIBLE_DEVICES\"] = \"\"  # GPU 사용 안함 (필요 시)\n",
    "os.environ['PYTHONHASHSEED'] = '42'\n",
    "\n",
    "\n",
    "# Constants and directory setup\n",
    "SAVED_MODELS_DIR = '/dev'\n",
    "if not os.path.exists(SAVED_MODELS_DIR):\n",
    "    os.makedirs(SAVED_MODELS_DIR)\n",
    "\n",
    "# Model settings\n",
    "network_depth = 2\n",
    "runs= 10\n",
    "batch_size = 64\n",
    "epochs = 100\n",
    "random_seed= 42\n",
    "input_path= '/dev'\n",
    "dataset= 'census-income-full-mixed-binarized'\n",
    "previous_model = None\n",
    "\n",
    "    \n",
    "# Prepare DevNet command\n",
    "devnet_command = (\n",
    "    f'python /dev/devnet_0.py '\n",
    "    f'--data_format 0 '\n",
    "    f'--network_depth {network_depth} '\n",
    "    f'--runs {runs} '\n",
    "    f'--batch_size {batch_size} '\n",
    "    f'--epochs {epochs} '\n",
    "    f'--output {SAVED_MODELS_DIR} '\n",
    "    f'--ramdn_seed {random_seed} '\n",
    "    f'--known_outliers 30 '\n",
    "    f'--cont_rate 0.02 '\n",
    "    f'--input_path {input_path}/ '\n",
    "    f'--data_set {dataset}'\n",
    ")\n",
    "\n",
    "        \n",
    "os.system(devnet_command)\n",
    "\n",
    "print(\"Training and testing completed.\")  "
   ]
